          <h6 class="font-light">JUGAAD</h6>
        </a>
      </div>
      <div class="md:flex flex-grow ml-4 my-auto hidden">
        {% for link in linklists['main-menu'].links %}
          <a href="{{ link.url }}" class="pr-4">
            <h5 class="text-sm font-light">{{ link.title | upcase }}</h5>
          </a>
        {% endfor %}
      </div>
      <div class="flex mr-2 flex-grow justify-end">
        <div class="navicons flex items-center">
          <a href="#" class="search_trigger px-2 sm:flex h-[30px] hidden">
            <div class="flex my-auto h-[25px] w-[100px] border-b-[1px] border-black items-center">
              <div class="w-[20px] h-[20px]">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  fill="none"
                  viewBox="0 0 24 24"
                  stroke-width="1.5"
                  stroke="currentColor"
                  class="w-[20px] h-[20px] my-auto"
                >
                  <path stroke-linecap="round" stroke-linejoin="round" d="M21 21l-5.197-5.197m0 0A7.5 7.5 0 105.196 5.196a7.5 7.5 0 0010.607 10.607z" />
                </svg>
              </div>
              <h6 class="pl-2 text-gray-400 flex-grow font-light">search</h6>
            </div>
          </a>
          {% comment %}
                account_icon
              <div clas="">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 24 24"
                stroke-width="1.5"
                stroke="currentColor"
                class="account_icon w-6 h-6 mr-2"
              >
                <path stroke-linecap="round" stroke-linejoin="round" d="M17.982 18.725A7.488 7.488 0 0012 15.75a7.488 7.488 0 00-5.982 2.975m11.963 0a9 9 0 10-11.963 0m11.963 0A8.966 8.966 0 0112 21a8.966 8.966 0 01-5.982-2.275M15 9.75a3 3 0 11-6 0 3 3 0 016 0z" />
              </svg>
            </div>
          {% endcomment %}

          <a
            href="#"
            data-drawer-trigger
            aria-controls="drawer-name"
            aria-expanded="false"
            class="relative h-[50px] flex items-center justify-center mr-1"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke-width="1.5"
              stroke="currentColor"
              class="cart_icon w-[24px] h-[24px] mr-2"
            >
              <path stroke-linecap="round" stroke-linejoin="round" d="M2.25 3h1.386c.51 0 .955.343 1.087.835l.383 1.437M7.5 14.25a3 3 0 00-3 3h15.75m-12.75-3h11.218c1.121-2.3 2.1-4.684 2.924-7.138a60.114 60.114 0 00-16.536-1.84M7.5 14.25L5.106 5.272M6 20.25a.75.75 0 11-1.5 0 .75.75 0 011.5 0zm12.75 0a.75.75 0 11-1.5 0 .75.75 0 011.5 0z" />
            </svg>
            <span
              class="cart_count hidden absolute top-2 right-0 h-fit bg-black text-white rounded-full p-1 px-2 text-[0.4rem] sm:text-[0.4rem] md:text-[0.6rem] font-extralight"
            ></span>
          </a>

          <a href="#" class="h-[50px] flex items-center justify-center">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke-width="1.5"
              stroke="currentColor"
              class="menu_icon w-[24px] h-[24px] mr-2 block md:hidden"
            >
              <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 9h16.5m-16.5 6.75h16.5" />
            </svg>
          </a>
        </div>
      </div>
    </nav>

    <section class="search_wrapper border-b-[1px] border-gray-500 hidden">
      <form action="{{ routes.search_url }}" method="get" class="flex-auto">
        <div class="w-screen p-4 flex items-center justify-center text-[0.6rem] sm:text-[0.8rem] md:text-[1rem]">
          <input
            type="search"
            name="q"
            class="w-8/12 border-b-[1px] border-gray-300 py-2 mr-2 pl-1 focus:border-b-0 focus:border-l-[1px] focus:ring-none focus:outline-none focus:ring-opacity-0 font-light"
            placeholder="what are you looking for?"
          >
          <div class="flex flex-grow">
            <button
              type="submit"
              class="px-6 sm:px-8 mt-2 flex items-center justify-center border border-gray-500 font-normal"
            >
              Search
            </button>
          </div>
          <div class="search_trigger_close mr-auto cursor-pointer">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke-width="1.5"
              stroke="currentColor"
              class="w-[24px] h-[24px]"
            >
              <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
            </svg>
          </div>
        </div>
      </form>
    </section>
    <script>
      document.querySelector(".search_trigger").addEventListener("click", (e) => {
        e.preventDefault();
        document.querySelector(".search_wrapper").classList.remove("hidden");
        document.querySelector(".search_trigger").classList.remove("sm:flex");
      });

      document.querySelector(".search_trigger_close").addEventListener("click", (e) => {
        e.preventDefault();
        document.querySelector(".search_wrapper").classList.add("hidden");
        document.querySelector(".search_trigger").classList.add("sm:flex");
      });
    </script>

    <main role="main">
      {{ content_for_layout }}
      {% section 'cart-drawer' %}
      <script src="https://cdn.jsdelivr.net/npm/axios@1.1.2/dist/axios.min.js"></script>
      {{ 'drawer.js' | asset_url | script_tag }}
    </main>
    {% section 'footer' %}
  </body>
</html>
